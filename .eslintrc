{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:react/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
      "no-console": "off"
    }
  }
      
  